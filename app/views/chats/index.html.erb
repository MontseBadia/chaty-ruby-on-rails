<ul>
  <% if @existing_chats_users.size != 0 %>
   <% @existing_chats_users.each do |other_user| %>
    <% if other_user != current_user %>
      <li>
        <%= link_to other_user.username, user_chats_path(current_user, :other_user => other_user.id), method: :post %>
      </li>
     <% end %>
   <% end %>
  <% else %>
    <p>No chats yet</p>
  <% end %>
</ul>

All Users:
<ul>
  <% User.all.each do |other_user| %>
   <% if other_user != current_user %>
     <li>
       <%= link_to(user_chats_path(current_user, :other_user => other_user.id), method: :post) do %>
       <%= other_user.username %>
       <% end %>
     </li>
    <% end %>
  <% end %>
</ul>
<%= link_to "Logout", session_path(current_user.id), method: :delete  %>