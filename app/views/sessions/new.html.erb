<%= form_for @user, url: sessions_path, method: :post do |f| %>
  <% flash.each do |k, value| %>
    <%= content_tag(:p, value) %>
  <% end %>
  <%= f.label :username %>
  <%= f.text_field :username %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.submit 'Login' %>
<% end %>
<%= link_to 'Or Register', new_user_path %>
